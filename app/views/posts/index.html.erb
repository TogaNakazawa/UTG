<section class="timeline-container">
  <div class="row">

    <div class="col-sm-6 offset-3 timeline">
      <% @posts.each do |t| %>
        <div class="post-container">

          <%= t.body %>
          <br>
          <%= t.created_at %>
        </div>
      <% end %>
    </div>
    <div class="col-sm-3">
    </div>
  </div>
</section>
